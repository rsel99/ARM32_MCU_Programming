.cpu cortex-m0
.thumb
.syntax unified
.fpu softvfp

.global login
login: .string "selvara2"
hello_str: .string "Hello, %s!\n"
.align 2
.global hello
hello:
	push {lr}
	ldr r2, =hello_str
	ldr r0, [r2]
	ldr r2, =login
	ldr r1, [r2]
	bl printf
	pop  {pc}

showmult2_str: .string "%d * %d = %d\n"
.align 2
.global showmult2
showmult2:
	push {r4-r7, lr}
	ldr r4, =showmult2_str
	movs r4, r0
	movs r5, r1
	movs r6, r4
	muls r6, r5

	ldr r7, =showmult2_str
	ldr r0, [r7]
	movs r1, r4
	movs r2, r5
	movs r3, r6

	bl printf
	pop  {r4-r7, pc}

// Add the rest of the subroutines here
.global showmult3
showmult3:
	push {lr}

	pop  {pc}

.global listing
listing:
	push {lr}

	pop  {pc}

.global trivial
trivial:
	push {lr}

	pop  {pc}

.global reverse_puts
reverse_puts:
	push {lr}

	pop  {pc}

.global sumsq
sumsq:
	push {lr}

	pop  {pc}
