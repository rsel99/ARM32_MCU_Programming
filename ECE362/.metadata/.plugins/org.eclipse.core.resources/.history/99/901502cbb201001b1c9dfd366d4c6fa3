.cpu cortex-m0
.thumb
.syntax unified
.fpu softvfp

.global login
login: .string "selvara2"
hello_str: .string "Hello, %s!\n"
.align 2
.global hello
hello:
	push {lr}
	ldr r0, =hello_str
	ldr r1, [r0]
	ldr r0, =login
	ldr r2, [r0]
	bl printf
	pop  {pc}

showmult2_str: .string "%d * %d = %d\n"
.align 2
.global showmult2
showmult2:
	push {lr}

	pop  {pc}

